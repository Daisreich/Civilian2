"VertexLitGeneric"
{
	"$baseTexture" "Models/Weapons/v_physcannon/v_superphyscannon_sheet"
	"$normalmap" "Models/Weapons/v_physcannon/v_superphyscannon_sheet_normal"
	"$phongexponenttexture" "Models/Weapons/v_physcannon/v_superphyscannon_sheet_ao"

	"$selfillum" 1

	"$phong" "1"
	"$phongexponent" "18"
	"$phongboost" "2"
	"$lightwarptexture" "models\weapons\lightwarp_tf2style\lightwarp_tf2style02"

	"$phongfresnelranges"	"[.5 .9 .3]"

	"$halflambert" "1"
	"$glowcolor" "2"

	"$tempvar" "0"

	"$commadd_phongexponent" "20"
	"$commadd_phongboost" 	"22.5"

	"$basemapalphaphongmask" "1"
	
	"$rimlight" "3"						
	"$rimlightexponent" "28"				
	"$rimlightboost" "3"

	"$yellow" "0"

	"$selfillum" "1"
	// "$selfillumtint" "[0.3 1.8 2.1]" // Default
	// "$selfillumtint" "[1.5 2.2 1.9]" // White
	// "$selfillumtint" "[3.5 0.3 0.2]" // Red
	// "$selfillumtint" "[2.8 1.7 0.4]" // Yellow
	// "$selfillumtint" "[0.5 2.1 0.8]" // Green (medical)
	// "$selfillumtint" "[1.0 2.4 0.4]" // Green (forest)
	// "$selfillumtint" "[0.5 0.6 3.1]" // Blue (lavender)
	// "$selfillumtint" "[1.4 0.6 2.5]" // Purple
	// "$selfillumtint" "[1.9 0.6 1.1]" // Pink

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		} 
		"PlayerWeaponColor"
		{
			"resultVar"	"$selfillumtint"
		}
	}
}